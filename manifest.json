{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Save page to Google Drive™",
	"description": "__MSG_description__",
	"version": "7.6.1",
	"background": {
		"scripts": [
			"js/jquery.js",
			"js/common.js",
			"js/checkerPlusForDrive.js",
			"js/difflib.js",
			"js/diffview.js",
			"js/uploadFile.js",
			"js/background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "images/buttonIcons/default.png",
		"default_title": "Checker Plus for Google Drive™",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/icon19.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"options_page": "options.html",
	"default_locale": "en",
	"permissions": [
		"http://*/*", "https://*/*", "file:///*",
		"alarms",
		"idle",
		"storage",
		"notifications",
		"contextMenus",
		"gcm",
		"identity",
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking"
	],
	"content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": false,
        "js": ["js/content.js"]
    }],
	"optional_permissions": [
		"background",
		"*://*.googleusercontent.com/*",
		"https://docs.google.com/"
	],
	"storage": {
		"managed_schema": "schema.json"
	},
	"oauth2": {
		"client_id": "305496705996-m5ifabehp2g7t5bv9n94m491flmbngv6.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/drive",
			"https://www.googleapis.com/auth/drive.readonly",
			"https://www.googleapis.com/auth/drive.photos.readonly"
		]
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://www.google-analytics.com https://www.gstatic.com; object-src 'self'",
	"web_accessible_resources": [
		"images/icon128.png",
		"images/driveIcons/*",
		"options.html",
		"popup.html",
		"contribute.html",
		"oauth2callback.html",
		"message-panel.html", 
		"unsaved-panel.html", 
		"comments-panel.html", 
		"pageinfo-panel.html",
		"pageinfo-bar-compressed.html", 
		"js/shadowloader-compressed.js", 
		"js/resourceloader-compressed.js"
	],
	"gcm_sender_id": "103953800507"
}